{
  "name": "react-next-admin",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev:wasm": "npm run build:wasm",
    "dev": "pnpm env:dev max dev",
    "build": "pnpm env:prod max build",
    "build:dev": "pnpm env:dev max build",
    "build:pre": "pnpm env:pre max build",
    "env:dev": "UMI_ENV=dev",
    "env:pre": "cross-env UMI_ENV=pre",
    "env:prod": "cross-env UMI_ENV=prod",
    "postinstall": "max setup && patch-package",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "locale:remove": "tsx script/locale-remove",
    "locale:translate": "tsx script/locale-translate",
    "pre": "pnpm env:pre max dev",
    "prepare": "husky install",
    "prettier:fix": "prettier --config .prettierrc.js --write 'src/**/*.{js,jsx,tsx,ts,md,json}'",
    "preview:pre": "pnpm env:pre max preview",
    "preview:prod": "pnpm env:prod max preview",
    "umi:clean": "rm -rf node_modules/.cache src/.umi dist",
    "umig": "max g",
    "build:wasm": "npm run umi:clean  && npm run watch",
    "watch": "cd wasm-lib && cargo watch -i .gitignore -i 'pkg/*' -s 'wasm-pack build --target web --out-dir pkg'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,md,json}": [
      "prettier  --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript  --write"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-components": "^2.7.1",
    "ahooks": "^3.7.11",
    "antd": "^5.17.0",
    "antd-img-crop": "^4.21.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "memoize-one": "^6.0.0",
    "react": "^18.2.0",
    "react-fast-marquee": "^1.6.4",
    "use-switch-tabs": "^0.2.3",
    "wasm-lib": "file:./wasm-lib/pkg"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@babel/runtime": "^7.24.5",
    "@lobehub/i18n-cli": "^1.18.1",
    "@milahu/patch-package": "^6.4.14",
    "@svgr/core": "^8.1.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@umijs/max": "^4.1.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.2.0",
    "eslint-plugin-valtio": "^0.6.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "react-dom": "^18.3.1",
    "react-router": "^6.23.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "overrides": {}
  }
}